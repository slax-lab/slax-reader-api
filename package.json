{
  "name": "slax-reader-api",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "deploy": "wrangler deploy --minify -c ./config/config.toml",
    "dev": "wrangler dev --env dev --ip 0.0.0.0 -c ./config/dev.toml --experimental-vectorize-bind-to-prod --test-scheduled",
    "preview": "wrangler dev --env preview --remote -c ./config/config.toml",
    "test": "vitest",
    "types": "wrangler types -c ./config/dev.toml",
    "lint": "eslint . --ext .mjs,.js,.vue,.ts",
    "lint:fix": "eslint . --ext .mjs,.js,.ts --fix",
    "lint:fix:gen": "eslint ./src/di/generated --ext .ts --fix",
    "gen:model": "prisma generate",
    "gen:pull": "prisma db pull",
    "gen:diff": "node ./script/gen-dao.js",
    "gen:di": "tsx ./script/gen-di.ts",
    "gen:cron": "tsx ./script/gen-cron.ts",
    "gen:consumer": "tsx ./script/gen-consumer.ts",
    "gen:router": "tsx ./script/gen-routers.ts",
    "gen:all": "pnpm gen:model && pnpm types && pnpm gen:cron && pnpm gen:consumer && pnpm gen:router && pnpm gen:di && pnpm prettier --write ./src/di/generated && pnpm prettier --write ./worker-configuration.d.ts",
    "migration:local": "wrangler d1 migrations apply slax-reader-backend --local -c ./config/dev.toml",
    "migration:local:fulltext": "wrangler d1 migrations apply slax-reader-backend-fulltext --local -c ./config/config.toml",
    "mcp:tool": "npx @modelcontextprotocol/inspector"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/azure": "^1.3.24",
    "@ai-sdk/google": "^1.2.22",
    "@ai-sdk/openai": "^1.3.23",
    "@ai-sdk/xai": "^1.2.18",
    "@cloudflare/puppeteer": "^1.0.2",
    "@modelcontextprotocol/inspector": "^0.15.0",
    "@modelcontextprotocol/sdk": "^1.15.0",
    "@prisma/adapter-d1": "^6.14.0",
    "@prisma/client": "^6.14.0",
    "@slax-lab/readability": "0.6.3",
    "@types/node": "^20.16.5",
    "@types/web": "^0.0.151",
    "@upstash/redis": "^1.35.3",
    "agents": "^0.0.101",
    "ai": "^4.3.19",
    "approx-string-match": "^2.0.0",
    "date-fns": "^3.6.0",
    "grammy": "^1.30.0",
    "hashids": "^2.3.0",
    "itty-router": "^5.0.22",
    "jose": "^5.9.1",
    "letterparser": "^0.1.7",
    "linkedom": "^0.18.11",
    "moment-timezone": "^0.5.45",
    "openai": "^5.10.1",
    "stripe": "^18.4.0",
    "zod": "^3.25.75"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.7.6",
    "@cloudflare/workers-types": "^4.20250813.0",
    "@edge-runtime/vm": "^5.0.0",
    "@types/nodemailer": "^6.4.17",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^7.18.0",
    "cloudflare": "^4.1.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.1.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3",
    "prisma": "^6.14.0",
    "ts-morph": "^25.0.1",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.26.0",
    "vitest": "^3.0.7",
    "wrangler": "^4.30.0"
  }
}
